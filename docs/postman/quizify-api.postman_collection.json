    {
      "name": "Points System Integration Tests",
      "item": [
        {
          "name": "Test Points Flow - Easy Quiz",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"responses\": [\n    {\n      \"question_id\": 1,\n      \"answer_id\": 1\n    },\n    {\n      \"question_id\": 2,\n      \"answer_id\": 3\n    },\n    {\n      \"question_id\": 3,\n      \"answer_id\": 5\n    },\n    {\n      \"question_id\": 4,\n      \"answer_id\": 7\n    },\n    {\n      \"question_id\": 5,\n      \"answer_id\": 9\n    }\n  ],\n  \"time_spent\": 180\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/quizzes/1/submit",
              "host": ["{{base_url}}"],
              "path": ["api", "quizzes", "1", "submit"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Perfect score on easy quiz gives base points', function () {",
                  "    var json = pm.response.json();",
                  "    pm.expect(json.score).to.equal(5);",
                  "    pm.expect(json.percentage).to.equal(100);",
                  "    pm.expect(json.points.level_multiplier).to.equal(1.0);",
                  "    pm.expect(json.points.performance_bonus).to.equal(50);",
                  "});"
                ]
              }
            }
          ]
        },
        {
          "name": "Test Points Flow - Hard Quiz",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"responses\": [\n    {\n      \"question_id\": 6,\n      \"answer_id\": 11\n    },\n    {\n      \"question_id\": 7,\n      \"answer_id\": 13\n    },\n    {\n      \"question_id\": 8,\n      \"answer_id\": 15\n    }\n  ],\n  \"time_spent\": 600\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/quizzes/3/submit",
              "host": ["{{base_url}}"],
              "path": ["api", "quizzes", "3", "submit"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Perfect score on hard quiz gives multiplied points', function () {",
                  "    var json = pm.response.json();",
                  "    if (json.score === 3) {",
                  "        pm.expect(json.points.level_multiplier).to.equal(2.0);",
                  "        pm.expect(json.points.level_points).to.be.above(json.points.base_points);",
                  "    }",
                  "});"
                ]
              }
            }
          ]
        },
        {
          "name": "Verify Total Points After Tests",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/points/user",
              "host": ["{{base_url}}"],
              "path": ["api", "points", "user"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('User points are accumulated correctly', function () {",
                  "    var json = pm.response.json();",
                  "    pm.expect(json.total_points).to.be.a('number');",
                  "    pm.expect(json.total_points).to.be.above(0);",
                  "    console.log('Total user points: ' + json.total_points);",
                  "});"
                ]
              }
            }
          ]
        }
      ]
